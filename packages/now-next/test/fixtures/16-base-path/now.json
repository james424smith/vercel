{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/next" }],
  "probes": [
    {
      "path": "/docs/_next/static/testing-build-id/pages/index.js",
      "responseHeaders": {
        "cache-control": "public,max-age=31536000,immutable"
      }
    },
    {
      "path": "/docs/",
      "mustContain": "hello from index"
    },
    {
      "path": "/docs",
      "mustContain": "hello from index"
    },
    {
      "path": "/docs/another",
      "mustContain": "hello from another"
    }
  ]
}
