{
  "version": 2,
  "builds": [
    { "src": "index.php", "use": "@now/php" },
    { "src": "subdirectory/index.php", "use": "@now/php" }
  ],
  "probes": [
    { "path": "/", "mustContain": "cow:RANDOMNESS_PLACEHOLDER" },
    { "path": "/subdirectory/", "mustContain": "yoda:RANDOMNESS_PLACEHOLDER" }
  ]
}
