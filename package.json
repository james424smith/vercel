{
  "name": "now",
  "version": "8.0.0-beta.2",
  "license": "Apache-2.0",
  "scripts": {
    "test": "eslint . && webpack",
    "prepublish": "webpack",
    "lint:staged": "lint-staged",
    "dev": "webpack -w",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write --single-quote --no-semi",
      "git add"
    ]
  },
  "main": "./dist/now.js",
  "files": [
    "dist"
  ],
  "bin": {
    "now": "./dist/now.js"
  },
  "eslintIgnore": [
    "src/providers/sh/legacy/**",
    "dist"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "script"
    },
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "func-names": [
        "error",
        "as-needed"
      ],
      "no-console": 0,
      "no-shadow": "error",
      "no-extra-semi": 0
    }
  },
  "babel": {
    "plugins": [
      "transform-flow-comments"
    ]
  },
  "dependencies": {
    "ansi-escapes": "2.0.0",
    "archiver": "2.0.0",
    "array-unique": "0.3.2",
    "async-retry": "1.1.3",
    "aws-sdk": "2.98.0",
    "bytes": "2.5.0",
    "chalk": "2.1.0",
    "clipboardy": "1.1.4",
    "convert-stream": "1.0.2",
    "debug": "3.0.0",
    "deployment-type": "1.0.1",
    "docker-file-parser": "1.0.2",
    "dotenv": "4.0.0",
    "download": "6.2.5",
    "email-prompt": "0.3.1",
    "email-validator": "1.1.1",
    "fs-extra": "4.0.1",
    "fs.promised": "3.0.0",
    "glob": "7.1.2",
    "ignore": "3.3.3",
    "ini": "1.3.4",
    "inquirer": "3.2.1",
    "is-url": "1.2.2",
    "minimist": "1.2.0",
    "mkdirp-promise": "5.0.1",
    "ms": "2.0.0",
    "node-fetch": "1.7.2",
    "opn": "5.1.0",
    "ora": "1.3.0",
    "pipe-streams-to-promise": "0.2.0",
    "progress": "2.0.0",
    "resumer": "0.0.0",
    "socket.io-client": "2.0.3",
    "split-array": "1.0.1",
    "strip-ansi": "4.0.0",
    "tar-fs": "1.15.3",
    "then-sleep": "1.0.1",
    "tmp-promise": "1.0.3",
    "uid-promise": "1.0.0"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-flow-comments": "6.22.0",
    "eslint": "4.4.1",
    "flow-babel-webpack-plugin": "1.1.0",
    "lint-staged": "4.0.3",
    "pre-commit": "1.2.2",
    "prettier": "1.5.3",
    "shebang-loader": "0.0.1",
    "webpack": "3.5.4",
    "webpack-node-externals": "1.6.0"
  }
}
