{
  "version": 2,
  "builds": [
    {
      "src": "**/*.js",
      "use": "@now/node",
      "config": { "maxLambdaSize": "40mb" }
    }
  ],
  "probes": [
    {
      "path": "/lighthouse",
      "mustContain": "lighthouse:RANDOMNESS_PLACEHOLDER"
    },
    {
      "path": "/screenshot",
      "mustContain": "screenshot:RANDOMNESS_PLACEHOLDER"
    }
  ]
}
