{
  "version": 2,
  "builds": [{ "src": "*.py", "use": "@now/python" }],
  "routes": [{ "src": "/%E6%82%A8%E5%A5%BD", "dest": "custom.py" }],
  "probes": [
    {
      "path": "/?%E6%82%A8%E5%A5%BD=/",
      "mustContain": "path:  query: {'您好': '/'}"
    },
    {
      "path": "/%E6%82%A8%E5%A5%BD?hello=/",
      "mustContain": "path: 您好 query: {'hello': '/'}"
    }
  ]
}
