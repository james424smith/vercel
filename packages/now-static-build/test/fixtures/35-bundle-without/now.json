{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "zeroConfig": true }
    }
  ],
  "build": {
    "env": {
      "BUNDLE_WITHOUT": "development:test"
    }
  },
  "probes": [
    { "path": "/jekyll.txt", "mustContain": "not found" },
    { "path": "/middleman.txt", "mustContain": "not found" },
    { "path": "/dimples.txt", "mustContain": "dimples" }
  ]
}
