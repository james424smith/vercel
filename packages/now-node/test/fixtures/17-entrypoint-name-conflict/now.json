{
  "version": 2,
  "builds": [{ "src": "*.js", "use": "@now/node" }],
  "probes": [
    {
      "path": "/helpers.js",
      "mustContain": "helpers:RANDOMNESS_PLACEHOLDER"
    },
    {
      "path": "/bridge.js",
      "mustContain": "bridge:RANDOMNESS_PLACEHOLDER"
    },
    {
      "path": "/launcher.js",
      "mustContain": "launcher:RANDOMNESS_PLACEHOLDER"
    }
  ]
}
